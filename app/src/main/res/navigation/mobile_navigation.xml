<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:labelVisibilityMode="labeled"
    app:startDestination="@+id/navigation_home">
    <fragment
        android:id="@+id/navigation_ar"
        android:name="com.example.ar_realestate.ARFragment"
        android:label="@string/title_ar"
        tools:layout="@layout/fragment_ar" />

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.ar_realestate.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.ar_realestate.AddAdvFragment"
        android:label="@string/title_addadvertisement"
        tools:layout="@layout/fragment_add_adv" />

    <fragment
        android:id="@+id/navigation_user"
        android:name="com.example.ar_realestate.LoginFragment"
        android:label="@string/title_user"
        tools:layout="@layout/fragment_login" />
</navigation>